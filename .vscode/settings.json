{
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "typescript",
            "autoFix": true
        },
        {
            "language": "typescriptreact",
            "autoFix": true
        }
    ],
    "files.associations": {
        "*.json": "jsonc"
    },
    "files.exclude": {
        "**/build": true,
        "**/dist": true,
        "**/docs": true,
        "**/lib": true,
        "**/temp": true
    }
}
